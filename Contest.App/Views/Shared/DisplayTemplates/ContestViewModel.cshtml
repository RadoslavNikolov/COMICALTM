@model Contests.App.Models.ViewModels.ContestViewModel

<div class="col-md-3 photo-contests-photo-wrapper">
    <div class="photo-contest-photo">
        <img src="@Model.WallpaperUrl" class="gallery-item" width="300" />
    </div>
    <div class="photo-contest-photographer">
        @Html.ActionLink(@Model.OrganizatorName, "Details", "Users", routeValues: null, htmlAttributes: new { title = "Photographer profile" })
        <div class="photo-contest-category">@Model.Category</div>
    </div>
</div>


